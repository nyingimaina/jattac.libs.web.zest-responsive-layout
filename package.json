{
  "name": "jattac.libs.web.zest-responsive-layout",
  "version": "2.0.1",
  "description": "A highly performant and flexible React component for building responsive layouts with a dynamic side pane and main content area. Features customizable desktop widths, bounce animations, and a configurable mobile breakpoint for seamless user experiences across devices.",
  "author": "Jattac",
  "license": "ISC",
  "homepage": "https://github.com/nyingimaina/jattac.libs.web.zest-responsive-layout#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nyingimaina/jattac.libs.web.zest-responsive-layout.git"
  },
  "bugs": {
    "url": "https://github.com/nyingimaina/jattac.libs.web.zest-responsive-layout/issues"
  },
  "main": "./dist/ZestResponsiveLayout.js",
  "module": "./dist/ZestResponsiveLayout.esm.js",
  "types": "./dist/ZestResponsiveLayout.d.ts",
  "exports": {
    ".": {
      "types": "./dist/ZestResponsiveLayout.d.ts",
      "import": "./dist/ZestResponsiveLayout.esm.js",
      "require": "./dist/ZestResponsiveLayout.js",
      "default": "./dist/ZestResponsiveLayout.esm.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "docs",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "responsive",
    "layout",
    "sidebar",
    "pane",
    "dynamic",
    "ui",
    "component",
    "flexbox",
    "css-modules",
    "animations",
    "typescript",
    "jattac",
    "zest",
    "performance",
    "ux",
    "dx"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.module.css"
  ],
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^25.3.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "cssnano": "^7.1.2",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.57.1",
    "rollup-plugin-dts": "^6.3.0",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0"
  }
}
